# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'
project 'Discoverer.xcodeproj'
 
use_frameworks!

target 'Example' do
  platform :macos, '10.10'
  pod 'SwiftLint'
end

target 'Discoverer_macOS' do
  platform :macos, '10.10'
  pod 'SwiftLint'

  # Pods for Discoverer

  target 'DiscovererTests_macOS' do
    # Pods for testing
  	inherit! :search_paths
  end

end

target 'Discoverer_iOS' do
  platform :ios, '8.0'
  pod 'SwiftLint'

  # Pods for Discoverer

  target 'DiscovererTests_iOS' do
    # Pods for testing
    inherit! :search_paths
  end

end

target 'Discoverer_tvOS' do
  platform :tvos, '9.0'
  pod 'SwiftLint'

  # Pods for Discoverer

  target 'DiscovererTests_tvOS' do
    # Pods for testing
    inherit! :search_paths
  end

end

target 'Discoverer_watchOS' do
  platform :watchos, '2.0'
  pod 'SwiftLint'

  # Pods for Discoverer
end
